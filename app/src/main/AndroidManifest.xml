<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mobi.fastcard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:name=".modules.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".modules.login_register.login.FragmentLogin"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".modules.slashscreen.ActivtySlashScreen"
            android:label="@string/title_activity_activty_slash_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".modules.login_register.ActivityLoginRegister"></activity>
    </application>

</manifest>
